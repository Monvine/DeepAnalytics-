# 环境变量配置示例
# 复制此文件为 .env 并填入真实的值

# DeepSeek API配置
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# B站Cookie配置
DEFAULT_COOKIE=your_bilibili_cookie_here

# 数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=your_password
DB_NAME=bilibili_analytics

# JWT密钥
JWT_SECRET_KEY=your_jwt_secret_key_here

# 其他配置
DEBUG=False
LOG_LEVEL=INFO 